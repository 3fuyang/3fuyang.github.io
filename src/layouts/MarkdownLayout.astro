---
import '@fontsource/geist-mono';

import BaseLayout from './BaseLayout.astro'

import './MarkdownLayout.css'

interface Props {
  title: string
  keywords?: string[]
  description?: string
}

const { title, keywords, description } = Astro.props
---

<BaseLayout {...{ title, keywords, description }}>
  <article class="prose mx-auto dark:prose-invert">
    <slot />
  </article>
</BaseLayout>
