    "rehype-toc": "^3.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^2.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "unocss": "^0.45.22",
    "vite": "^3.1.3",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-pwa": "^0.13.1"
  }
