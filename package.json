{
  "name": "flog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm ./server.ts --trace-warnings",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": "tsc && vite build --outDir dist/server --ssr src/entry-server.tsx",
