{
  "extends": ["eslint:recommended", "plugin:astro/recommended"],
  "overrides": [
    {
      "files": ["*.astro"],
      "parser": "astro-eslint-parser",
      "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "extraFileExtensions": [".astro"]
      },
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx", "*.mjs"],
      "parser": "@typescript-eslint/parser",
      "extends": ["react-app", "plugin:@typescript-eslint/recommended"]
    }
  ]
}
